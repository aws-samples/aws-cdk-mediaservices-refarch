    "@typescript-eslint/parser": "^5.40.1",
    "aws-cdk": "2.87.0",
    "aws-cdk-lib": "2.87.0",
    "cdk-nag": "2.21.50",
    "constructs": "^10.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-array-func": "^3.1.7",
